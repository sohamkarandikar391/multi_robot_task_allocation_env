    </link>

    <link name="rotor_3">
      <pose>0.057 -0.057 0.02 0 0 0</pose>
      <inertial>
        <mass>0.001</mass>
        <inertia>
          <ixx>0.0000001</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.0000001</iyy>
          <iyz>0</iyz>
          <izz>0.0000001</izz>
        </inertia>
      </inertial>
      <visual name="visual">
        <geometry>
          <cylinder>
