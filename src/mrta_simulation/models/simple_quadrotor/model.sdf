<?xml version="1.0"?>
<sdf version="1.9">
  <model name="simple_quadrotor">
    <static>false</static>
    
    <link name="base_link">
      <pose>0 0 0 0 0 0</pose>
      
      <inertial>
        <mass>1.0</mass>
        <inertia>
          <ixx>0.1</ixx> <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.1</iyy> <iyz>0.0</iyz>
          <izz>0.1</izz> </inertia>
